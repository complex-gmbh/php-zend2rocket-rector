{
    "name": "complex/php-zend2rocket-rector",
    "description": "automated refactoring with rector to convert Zendcontrollers to Rocketcontrollers",
    "type": "library",
    "authors": [
        {
            "name": "Luca Feiser",
            "email": "l.feiser@complex-it.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "Complex\\Zend2RocketRector\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Complex\\Zend2RocketRector\\Tests\\Rector\\Controller\\": "tests/Rector/Controller"
        }
    },
    "require": {
        "rector/rector": "0.7.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
